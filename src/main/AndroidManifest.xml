<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.roadtestings1"
  xmlns:android="http://schemas.android.com/apk/res/android">

  <uses-permission android:name="android.permission.INTERNET" />
  <uses-permission
    android:name="android.permission.WRITE_EXTERNAL_STORAGE"
    android:maxSdkVersion="28" />
  <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

  <application
    android:requestLegacyExternalStorage="true"
    android:allowBackup="true"
    android:icon="@drawable/road"
    android:label="@string/app_name"
    android:roundIcon="@drawable/road"
    android:supportsRtl="true"
    android:theme="@style/Theme.RoadTestings1">
    <activity
      android:name=".MainActivity"
      android:exported="true">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".Gradation.GSB.Gradation_5.Gradation5InputActivity"/>
    <activity android:name=".Gradation.GSB.Gradation_5.Gradation5OutputActivity"
      android:screenOrientation="landscape" />
    <activity android:name=".Gradation.GSB.GradationTypesActivity"/>
    <activity android:name=".Gradation.WMM.GRBInputActivity"/>
    <activity android:name=".Gradation.WMM.GRBOutputActivity"
      android:screenOrientation="landscape"/>
    <activity android:name=".Gradation.GSB.Gradation_1.Gradation1InputActivity"/>
    <activity android:name=".Gradation.GSB.Gradation_1.Gradation1OutputActivity"
      android:screenOrientation="landscape"/>
    <activity android:name=".Gradation.GSB.Gradation_3.Gradation3InputActivity"/>
    <activity android:name=".Gradation.GSB.Gradation_3.Gradation3OutputActivity"
      android:screenOrientation="landscape"/>
    <activity android:name=".Gradation.InsideGradationActivity"/>
    <activity android:name=".Density.DensityInputActivity"/>
    <activity android:name=".Density.DensityOutputActivity"/>
  </application>

</manifest>
